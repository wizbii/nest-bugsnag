{
  "name": "@wizbii/nest-bugsnag",
  "version": "9.0.0",
  "description": "A Nest module wrapper for bugsnag",
  "keywords": [
    "nestjs",
    "bugsnag",
    "logger"
  ],
  "engines": {
    "node": "16.x"
  },
  "url": "https://github.com/wizbii/nest-bugsnag",
  "homepage": "https://github.com/wizbii/nest-bugsnag",
  "bugs": {
    "url": "https://github.com/wizbii/nest-bugsnag/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wizbii/nest-bugsnag.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Wizbii",
  "license": "MIT",
  "scripts": {
    "clean:build": "yarn clean && yarn build",
    "build": "tsc -p tsconfig.json && ts-node postbuild.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn clean:build",
    "test": "jest --config jest.config.json",
    "lint": "run-p -c 'lint:*'",
    "lint:pretty": "prettier './**/*.{ts,scss,js,json,yml,md,html}' --list-different",
    "lint:src": "eslint 'src/**/**.ts' --ignore-pattern 'src/**/*.spec.ts'",
    "lint:spec": "eslint 'test/**/**.spec.ts' -c '.eslintrc.spec.js'"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bugsnag/core": "^7.18.0",
    "@bugsnag/js": "^7.18.0",
    "@bugsnag/plugin-express": "^7.18.0",
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.2.0",
    "@types/jest": "^29.2.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8"
  },
  "peerDependencies": {
    "@bugsnag/core": "^7.18.0",
    "@bugsnag/js": "^7.18.0",
    "@bugsnag/plugin-express": "^7.18.0",
    "@nestjs/common": "^9.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.1.0"
  }
}
