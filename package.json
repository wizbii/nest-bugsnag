{
  "name": "@wizbii/nest-bugsnag",
  "version": "7.0.0",
  "description": "A Nest module wrapper for bugsnag",
  "keywords": [
    "nestjs",
    "bugsnag",
    "logger"
  ],
  "engines": {
    "node": "14.x"
  },
  "url": "https://github.com/wizbii/nest-bugsnag",
  "homepage": "https://github.com/wizbii/nest-bugsnag",
  "bugs": {
    "url": "https://github.com/wizbii/nest-bugsnag/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Wizbii",
  "license": "MIT",
  "scripts": {
    "clean:build": "yarn clean && yarn build",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "postbuild": "ts-node postbuild.ts",
    "prepublishOnly": "yarn clean:build",
    "test": "jest --config jest.config.json",
    "lint": "run-p -c lint:*",
    "lint:pretty": "prettier './**/*.{ts,scss,js,json,yml,md,html}' --list-different",
    "lint:src": "eslint 'src/**/**.ts' --ignore-pattern 'src/**/*.spec.ts'",
    "lint:spec": "eslint 'test/**/**.spec.ts' -c '.eslintrc.spec.js'"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bugsnag/core": "^7.10.0",
    "@bugsnag/js": "^7.10.0",
    "@bugsnag/plugin-express": "^7.10.0",
    "@nestjs/common": "^7.6.17",
    "@nestjs/core": "^7.6.17",
    "@nestjs/platform-express": "^7.6.17",
    "reflect-metadata": "0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "6.6.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.17",
    "@types/jest": "^26.0.23",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "jest": "^26.6.3",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@bugsnag/core": "^7.10.0",
    "@bugsnag/js": "^7.10.0",
    "@bugsnag/plugin-express": "^7.10.0",
    "@nestjs/common": "^7.6.17",
    "@nestjs/platform-express": "^7.6.17",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3"
  }
}
